<?xml version="1.0" encoding="UTF-8"?>
<codeBook xsi:schemaLocation="ddi:codebook:2_5 http://www.ddialliance.org/Specification/DDI-Codebook/2.5/XMLSchema/codebook.xsd" version="2.5" xmlns="ddi:codebook:2_5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<docDscr>
		<citation>
			<titlStmt>
				<titl>DDI2.5 XML CODEBOOK</titl>
			</titlStmt>
		</citation>
	</docDscr>
	<stdyDscr>
		<citation>
			<titlStmt>
				<titl>{{titl}}
				</titl>
				<altTitl/>
			</titlStmt>
			<rspStmt>
				{{#AuthEnty}}
				<AuthEnty>{{.}}</AuthEnty>
				{{/AuthEnty}}
				<othId/>
			</rspStmt>
			<prodStmt>
				{{#producer}}
				<producer>{{.}} </producer>
				{{/producer}}
				<copyright>{{copyright}}</copyright>
				<fundAg>{{fundAg}}</fundAg>
				{{#grantNo}}
				<grantNo>{{.}}</grantNo>
				{{/grantNo}}			
			</prodStmt>
			<distStmt>
				{{#depositr}}
				<depositr>{{.}}</depositr>
				{{/depositr}}
				<distDate></distDate>
			</distStmt>
			<serStmt>
				<serName>{{serName}}</serName>
			</serStmt>
			<verStmt>
				<version></version>
			</verStmt>
		</citation>
		<stdyInfo>
			<subject>
				{{#keyword}}
				<keyword>{{.}}</keyword>
				{{/keyword}}
				{{#topcClas}}
				<topcClas>{{.}}</topcClas>
				{{/topcClas}}
			</subject>
			<abstract>{{abstract}}
			</abstract>
			<sumDscr>
				<timePrd>{{timePrd}}</timePrd>
				<collDate>{{collDate}}</collDate>
				{{#nation}}
				<nation>{{.}}</nation>
				{{/nation}}
				{{#geogCover}}
				<geogCover>{{.}}</geogCover>
				{{/geogCover}}
				{{#geogUnit}}
				<geogUnit>{{.}}</geogUnit>
				{{/geogUnit}}
				{{#anlyUnit}}
				<anlyUnit>{{.}}</anlyUnit>
				{{/anlyUnit}}
				<universe/>
				{{#dataKind}}
				<dataKind>{{.}}</dataKind>
				{{/dataKind}}
			</sumDscr>
		</stdyInfo>
		<method>
			<dataColl>
				<timeMeth/>
				{{#dataCollector}}
				<dataCollector>{{.}}</dataCollector>
				{{/dataCollector}}
				<sampProc/>
				<deviat/>
				<collMode/>
				<sources>
					{{#dataSrc}}
					<dataSrc>{{.}}</dataSrc>
					{{/dataSrc}}
				</sources>
				<weight/>
			</dataColl>
		</method>
		<dataAccs>
			<setAvail>
				{{#accsPlac}}
				<accsPlac>{{.}}</accsPlac>
				{{/accsPlac}}
			</setAvail>
			<useStmt>
				<restrctn/>
				<contact>{{contact}}</contact>
			</useStmt>
		</dataAccs>
		<othrStdyMat>
			<relStdy/>
			{{#relPubl}}
			<relPubl>{{.}}</relPubl>
			{{/relPubl}}
			<othRefs/>
		</othrStdyMat>
	</stdyDscr>
</codeBook>
